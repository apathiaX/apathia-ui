{
  "name": "apathia",
  "version": "1.0.0",
  "description": "Apathia Repository",
  "repository": {
    "type": "git",
    "url": "git@github.com:apathia1220/apathia-vue.git"
  },
  "scripts": {
    "build": "pnpm run -C internal start",
    "clear": "rimraf 'dist' && rimraf 'packages/*/{lib,node_modules}' && rimraf node_modules",
    "createNew": "node scripts/create.js",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm build && pnpm release:only",
    "release:only": "changeset publish --registry=https://registry.npmjs.org/",
    "lint": "eslint --no-fix --ext .jsx,.js,.ts,.tsx,.vue packages example scripts",
    "lint-fix": "eslint --fix --ext .jsx,.js,.ts,.tsx,.vue packages example scripts",
    "lint-staged": "lint-staged",
    "commitlint": "commitlint",
    "postinstall": "husky install",
    "docs:dev": "pnpm run -C docs dev",
    "docs:build": "pnpm run -C docs build",
    "docs:serve": "pnpm run -C docs serve",
    "example:dev": "pnpm run -C example dev",
    "cz": "git cz"
  },
  "lint-staged": {
    "*.{ts,vue,js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "vue",
    "typescript",
    "component"
  ],
  "author": "xzc01",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/element-resize-detector": "^1.1.3",
    "@types/fs-extra": "^11.0.4",
    "@types/gulp": "^4.0.17",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.17",
    "@types/string-width": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-vue": "^1.10.2",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/compiler-sfc": "^3.4.15",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "commitlint": "^17.4.2",
    "consola": "^2.15.3",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^9.9.0",
    "fs-extra": "^9.1.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "inquirer": "8.0.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-typescript2": "^0.32.1",
    "sass": "^1.57.1",
    "tslib": "^2.5.0",
    "typescript": "5.3.3",
    "vite": "^2.9.15",
    "vue": "^3.4.15"
  },
  "dependencies": {
    "@apathia/components": "workspace:^1.0.0",
    "@apathia/icons-vue": "workspace:^",
    "@apathia/shared": "workspace:^",
    "@apathia/theme": "workspace:^1.0.0",
    "@jridgewell/sourcemap-codec": "^1.4.14",
    "@popperjs/core": "^2.11.6",
    "element-resize-detector": "^1.2.4",
    "lodash-es": "^4.17.21",
    "string-width": "4.2.3",
    "twind": "^0.16.19",
    "vue-router": "^4.1.6"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite",
        "react",
        "react-dom"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/git-cz"
    }
  }
}